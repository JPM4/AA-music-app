<h1>Create a new band:</h1>

<form action="<%= bands_url %>" method="post">
  <input
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token %>">

  <label for "band_name">Band Name
    <input type="text" name="band[name]" id="name">
  </label>
  <br>

  <input type="submit">
</form>
